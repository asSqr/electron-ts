{
  "name": "electron-ts",
  "version": "1.0.0",
  "main": "main.js",
  "author": "As_sqr",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist/main.js",
    "build": "run-s build:main build:render",
    "build:main": "tsc",
    "build:render": "webpack",
    "test": "jest",
    "package:mac": "webpack && electron-builder --mac --x64",
    "package:windows": "webpack && electron-builder --win --x64",
    "package:linux": "webpack && electron-builder --linux --x64"
  },
  "build": {
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "productName": "TODO",
    "appId": "todo.example.com",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*.js",
      "./index.html",
      "./main.js"
    ],
    "dmg": {
      "contents": [{
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    }
  },
  "dependencies": {
    "css-loader": "^5.2.4",
    "eslint-loader": "^4.0.2",
    "fs-extra": "^10.0.0",
